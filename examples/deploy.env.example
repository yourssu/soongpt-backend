# Deploy runtime example (used by docker-compose.deploy.yml)
SERVER_PORT=8080
PROJECT_NAME=soongpt
ENVIRONMENT=dev

# ECR
ECR_REGISTRY=public.ecr.aws/<registry-id>
IMAGE_TAG=latest

# Database
DB_URL=jdbc:mysql://<db-host>:3306/soongpt?serverTimezone=Asia/Seoul
DB_USERNAME=<db-user>
DB_PASSWORD=<db-password>

# CORS
CORS_ALLOWED_ORIGIN=https://<frontend-domain>

# Slack
SLACK_CHANNEL=<slack-channel>
SLACK_LOG_CHANNEL=<slack-log-channel>
SLACK_TOKEN=<slack-token>

# Admin
ADMIN_PASSWORD=<admin-password>

# SSO
SSO_FRONTEND_URL=https://<frontend-domain>
SSO_CLIENT_JWT_SECRET=<jwt-secret>
SSO_ALLOWED_REDIRECT_URLS=https://<frontend-domain>

# Rusaint internal
RUSAINT_PSEUDONYM_SECRET=<pseudonym-secret>
RUSAINT_INTERNAL_JWT_SECRET=<internal-jwt-secret>
